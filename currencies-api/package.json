{
  "name": "currencies-api",
  "version": "1.0.0",
  "description": "Currencies api",
  "main": "./src/server.ts",
  "scripts": {
    "tsc": "tsc",
    "recreateDb.test": "cross-env NODE_ENV=test npx ts-node ./utils/recreateDb.ts",
    "start.dev": "cross-env NODE_ENV=dev node -r ts-node/register ./src/server.ts",
    "start.test": "cross-env NODE_ENV=test node -r ts-node/register ./src/server.ts",
    "start.dev.watch": "cross-env NODE_ENV=dev nodemon --config nodemon.dev.json -r dotenv/config sequelize-config.js",
    "start.test.watch": "cross-env NODE_ENV=test npm run recreateDb.test && nodemon --config nodemon.test.json -r dotenv/config sequelize-config.js",
    "build": "tsc --build ./tsconfig.build.json",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed.test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all"
  },
